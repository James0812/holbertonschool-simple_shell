#include <stdio.h>

/**
 * main - prints the program name
 * @argc: number of command line arguments
 * @argv: array containing the arguments
 *
 * Return: Always 0 (Success)
 */
int main(int argc, char *argv[])
{
	(void)argc; /* We don't use argc in this program */
	printf("%s\n", argv[0]);
	return (0);
}
